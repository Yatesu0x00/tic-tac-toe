<Window x:Class="ticTacToe.endWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ticTacToe"
        mc:Ignorable="d"
        Title="endWindow" Height="121" Width="225" WindowStartupLocation="CenterScreen">
    <Canvas Margin="0,0,2,-21">
        <Label Content="Noch ein weiteres Spiel?" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="15" FontFamily="Verdana" Canvas.Top="10" Width="200" Canvas.Left="10"/>
        <Button x:Name="bt_n" Content="Nein" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Click="bt_n_Click" FontSize="15" FontFamily="Verdana" Canvas.Left="120" Canvas.Top="48"/>
        <Button x:Name="bt_j" Content="Ja" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" RenderTransformOrigin="0.853,0.333" Click="bt_j_Click" FontSize="15" FontFamily="Verdana" Canvas.Left="10" Canvas.Top="48"/>

    </Canvas>
</Window>
